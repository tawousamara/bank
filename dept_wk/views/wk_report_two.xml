<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="test1_form" model="ir.ui.view">
        <field name="name">wk.report.two.form</field>
        <field name="model">wk.report.two</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="date_debut" />
                        <field name="date_fin" />
                    </group>
                    <field name="line_ids" readonly="1">
                        <tree>
                            <field name="management" />
                            <field name="code" />
                            <field name="employee_name" />
                            <field name="processed_file_num" />
                            <field name="processed_file_avrg" />
                            <field name="processed_file_prg" />
                        </tree>
                    </field>    
                </sheet>    
            </form>
        </field>
    </record>
    
    <record id="test1_tree" model="ir.ui.view">
        <field name="name">wk.report.two.tree</field>
        <field name="model">wk.report.two</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="date_debut" />
                <field name="date_fin" />
            </tree>
        </field>
    </record>

    <record id="wk_report_two" model="ir.actions.act_window">
        <field name="name">التقرير 2</field>
        <field name="res_model">wk.report.two</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>