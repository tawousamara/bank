<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Personal Document -->
        <record id="personal_folder" model="documents.folders">
            <field name="name">Personal Document</field>
            <field name="is_personal_directory">True</field>
        </record>

        <!-- Shared Document -->
        <record id="portal_folder" model="documents.folders">
            <field name="name">Portal Document</field>
            <field name="is_portal_directory">True</field>
        </record>

    </data>
</odoo>
